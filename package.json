{
  "name": "shp2json-js",
  "version": "0.1.0",
  "description": "Converts shapefiles to GeoJSON, all in JavaScript",
  "main": "lib/index.js",
  "repository": {
    "type": "git",
    "url": "git://github.com/patosai/shp2json-js.git"
  },
  "scripts": {
    "test": "mocha ./test/test.js",
    "hint": "jshint ./lib/*.js",
    "build-test": "browserify ./test/test.js > ./test/bundle.js",
    "build": "browserify . -s shp > ./dist/shp.js",
    "min": "browserify . -s shp | uglifyjs -mc > ./dist/shp.min.js"
  },
  "author": "Patrick Tsai",
  "license": "MIT",
  "readmeFilename": "README.md",
  "devDependencies": {
    "browserify": "^13.0.1",
    "chai": "^3.5.0",
    "chai-as-promised": "^5.3.0",
    "mocha": "^2.4.5",
    "testling": "^1.7.1",
    "uglifyjs": "^2.4.10"
  },
  "dependencies": {
    "jszip": "^3.0.0",
    "lie": "^3.0.4",
    "lru-cache": "^4.0.1",
    "parsedbf": "^0.1.2",
    "proj4": "^2.3.14"
  },
  "browser": {
    "./lib/binaryajax.js": "./lib/binaryajax-browser.js"
  }
}
